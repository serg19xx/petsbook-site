{
  "name": "petsbook-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "dev:ngrok": "node scripts/dev-ngrok.js",
    "dev:vite-ngrok": "npx dotenv -e .env.ngrok -- vite",
    "dev:all": "npm-run-all --parallel dev:vite-ngrok dev:ngrok",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run ngrok\"",
    "ngrok": "ngrok http 5173"
  },

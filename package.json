    "@vuelidate/validators": "^2.0.0",
    "axios": "^1.8.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "ieee754": "^1.2.1",
    "jwt-decode": "^4.0.0",
    "maska": "^2.1.10",
    "picomatch": "^4.0.2",
    "pinia": "^3.0.1",
    "protocol-buffers-schema": "^3.6.0",
    "resolve-protobuf-schema": "^2.1.0",
    "serialize-to-js": "^3.1.2",
